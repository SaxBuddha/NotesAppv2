<%- include("header") -%>

<h1>Delete a Note?</h1>

<form action="/delete/<%= model.Note_ID %>" method="post">
  <div class="form-horizontal">

    <%- include("display") -%>

    <div class="form-group row">
      <label class="col-form-label col-sm-2"></label>
      <div class="col-sm-10">
        <input type="submit" value="Delete" class="btn btn-default btn-danger" />
        <a class="btn btn-outline-dark cancel" href="/notes">Cancel</a>
      </div>
    </div>
  </div>
</form>

<%- include("footer") -%>